      <xs:element name="requestType" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="AVAIL_SPECTRUM_RESP">
    <xs:sequence>
      <xs:element name="message" type="xs:string" minOccurs="0"/>
      <xs:element name="timestamp" type="xs:string"/>
      <xs:element name="deviceDesc" type="ns1:DeviceDescriptor"/>
      <xs:element name="databaseChange" type="ns1:DbUpdateSpec" minOccurs="0"/>
      <xs:element name="rulesetInfo" type="ns1:RulesetInfo"/>
      <xs:element name="timeRange" type="ns1:EventTime"/>
      <xs:element name="needsSpectrumReport" type="xs:boolean" minOccurs="0"/>
      <xs:element name="position" type="xs:string" minOccurs="0"/>
      <xs:element name="channels" type="ns3:PawsChannel" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
