      <xs:enumeration value="ETSI-EN-301-598-2.1.1"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="antennaHeightType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="AGL"/>
      <xs:enumeration value="AMSL"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="antennaPolarizationType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="H"/>
      <xs:enumeration value="V"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="etsiChannelRestrictionType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
    </xs:restriction>
  </xs:simpleType>
